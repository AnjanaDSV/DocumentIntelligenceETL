{
    "version": "0.2.0",
    "configurations": [
        {"name":"Python Debugger: Flask","type":"debugpy","request":"launch","module":"flask","env":{"FLASK_APP":"app.py","FLASK_DEBUG":"1"},"args":["run","--no-debugger","--no-reload"],"jinja":true,"autoStartBrowser":false},
        {
            "name": "Python: Run Flask App",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/insurance_dashboard/run.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "args": [
                "run"
            ],
            "justMyCode": true
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}
